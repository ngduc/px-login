{
  "name": "px-login",
  "version": "0.1.0",
  "description": "px-login - React minimal reusable Login Component.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "prepare": "cp ./src/index.tsx ./src/index.js",
    "build": "npm run prepare && nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@types/react": "^16.4.14",
    "awesome-typescript-loader": "^5.2.1",
    "nwb": "0.23.x",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "typescript": "^3.0.3"
  },
  "author": "Duc Nguyen",
  "homepage": "https://github.com/ngduc/px-login",
  "license": "MIT",
  "repository": "https://github.com/ngduc/px-login.git",
  "keywords": [
    "react-component"
  ]
}
